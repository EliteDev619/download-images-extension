{
    "action": {
       "default_icon": "icon.png",
       "default_popup": "popup.html"
    },
    "background": {
       "service_worker": "fixR.js"
    },
    "commands": {
       "_execute_action": {
          "suggested_key": {
             "default": "Ctrl+Shift+Y",
             "mac": "Command+Shift+Y"
          }
       }
    },
    "default_locale": "en",
    "description": "Download images from any website with one click.",
    "host_permissions": [ "http://*/*", "https://*/*", "\u003Call_urls>" ],
    "icons": {
       "128": "icon.png"
    },
    "manifest_version": 3,
    "name": "Download Images - FastImages",
    "permissions": [ "activeTab", "downloads", "storage", "webRequest", "scripting", "declarativeNetRequest" ],
    "short_name": "FastImages",
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "1.0.0"
 }